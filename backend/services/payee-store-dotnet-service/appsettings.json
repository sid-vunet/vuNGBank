{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Elastic.Apm": "Information"
    }
  },
  "AllowedHosts": "*",
  "ElasticApm": {
    "LogLevel": "Information",
    "CaptureHeaders": true,
    "CaptureBody": "all",
    "CentralConfig": false,
    "TransactionSampleRate": 1.0,
    "SpanFramesMinDuration": "5ms",
    "StackTraceLimit": 50
  }
}