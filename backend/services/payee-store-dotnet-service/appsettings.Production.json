{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Elastic.Apm": "Information"
    }
  },
  "ElasticApm": {
    "LogLevel": "Information",
    "CaptureHeaders": false,
    "CaptureBody": "errors",
    "CentralConfig": false,
    "TransactionSampleRate": 0.5,
    "SpanFramesMinDuration": "10ms",
    "StackTraceLimit": 30
  }
}